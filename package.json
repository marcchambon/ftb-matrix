{
  "name": "ftb-matrix",
  "version": "0.0.7",
  "description": "low-footprint matrix library for modern browser, which mimics ThreeJs matrix library",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run build && node --expose-gc --experimental-wasm-simd --harmony-weak-refs test/test.js",
    "build": "node --experimental-wasm-simd compileWasm.js && rollup --config"
  },
  "keywords": [
    "wasm",
    "matrix",
    "threejs",
    "speed",
    "webassembly",
    "webgl"
  ],
  "author": "Marc Chambon",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-wasm": "^5.0.0",
    "rollup": "^2.11.2",
    "rollup-plugin-terser": "^6.1.0",
    "tape": "^5.0.1",
    "wabt": "^1.0.16"
  }
}
